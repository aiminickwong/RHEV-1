      <web-uri>ovirtengineweb.war</web-uri>
      <context-root>/OvirtEngineWeb</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>restapi.war</web-uri>
      <context-root>/api</context-root>
    </web>
  </module>
  <module>
    <web>
      <web-uri>userportal.war</web-uri>
      <context-root>/UserPortal</context-root>
